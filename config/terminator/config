  suppress_multiple_term_dialog = True
[keybindings]
  close_window = <Primary><Shift>q
  split_horiz = <Alt>h
  close_term = <Primary>w
  split_vert = <Alt>v
  new_tab = <Primary>t
  copy = <Primary>c
  paste = <Primary>v
